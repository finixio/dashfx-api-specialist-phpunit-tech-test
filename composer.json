{
  "name": "finixio/dashfx-api-specialist-phpunit-tech-test",
  "description": "API specialist phpunit technical test",
  "authors": [
    {
      "name": "James Thatcher",
      "email": "james@finixio.com"
    }
  ],
  "type": "project",
  "require": {
    "php": "^8.0",
    "guzzlehttp/guzzle": "~6.0",
    "vlucas/phpdotenv": "^5.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpspec/prophecy-phpunit": "^2.0",
    "friendsofphp/php-cs-fixer": "^v3.1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "cs-fixer": "vendor/bin/php-cs-fixer fix --config=.php-cs-fixer.dist.php"
  },
  "minimum-stability": "stable"
}
